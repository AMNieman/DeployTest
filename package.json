    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2",
    "typescript": "~2.0.3",
    "systemjs": "0.19.27",
    "reflect-metadata": "^0.1.3",
    "@angular/cli": "1.0.0-beta.30"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
